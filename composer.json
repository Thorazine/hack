{
    "name": "thorazine/hack",
    "description": "A non-intrusive packaged CMS built on Laravel",
    "keywords": ["laravel", "hack", "cms"],
    "type": "project",
    "license": "MIT",
    "require": {
        "laravel/framework": "5.3.*|^5.4.13",
        "ext-gd": "*",
        "ext-exif": "*",
        "laravelcollective/html": "5.*",
        "intervention/image": "^2.3",
        "cartalyst/sentinel": "^2.0",
        "jenssegers/agent": "^2.5",
        "barryvdh/laravel-debugbar": "^2.3",
        "maatwebsite/excel": "^2.1",
        "thorazine/location": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Thorazine\\Hack\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Thorazine\\Hack\\HackServiceProvider",
                "Thorazine\\Hack\\Providers\\RouteServiceProvider"

            ],
            "aliases": {
                "Location": "Thorazine\\Hack\\Facades\\LocationFacade"
            }
        }
    }
}
